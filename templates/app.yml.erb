rackup: <%= @rackup %>
log: <%= scope.lookupvar('thin::params::log_dir') %>/<%= @name %>.log
pid: <%= scope.lookupvar('thin::params::pid_dir') %>/<%= @name %>.pid
daemonize: <%= @daemonize %>
